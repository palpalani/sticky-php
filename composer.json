{
  "name": "palpalani/sticky-php",
  "description": "Sticky Subscription Commerce Platform PHP Client",
  "require": {
    "php": "^8.2",
    "guzzlehttp/guzzle": "^7.5"
  },
  "require-dev": {
    "laravel/pint": "^1.0",
    "phpunit/phpunit": "^9.5|^10.0|^11.0|^12.0",
    "mockery/mockery": "^1.5",
    "fakerphp/faker": "^1.20",
    "phpstan/extension-installer": "^1.1",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-phpunit": "^1.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Kevin Em",
      "email": "kevinem@thinkdevise.com"
    },
    {
      "name": "palPalani",
      "email": "palani.p@gmail.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "KevinEm\\LimeLightCRM\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "KevinEm\\LimeLightCRM\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "analyse": "vendor/bin/phpstan analyse",
    "test": "vendor/bin/pest",
    "test-coverage": "vendor/bin/pest --coverage",
    "format": "vendor/bin/pint"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    }
  }
}
